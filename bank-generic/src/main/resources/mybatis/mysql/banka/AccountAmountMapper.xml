<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.netease.kaola.generic.api.mapper.AccountAmountMapper">

    <sql id="base_column">
        id,name
    </sql>

    <insert id="insertTest">
        INSERT INTO spring_test_account
        (<include refid="base_column"/>)
        VALUES (#{id},#{name})
    </insert>

    <select id="selectAccountAmount" resultType="Map">
        SELECT 1
    </select>

    <update id="deductionAccountAmountRecords">
        SELECT  1
    </update>

    <update id="realDeductionAccountAmountRecords">
        SELECT 1
    </update>
</mapper>