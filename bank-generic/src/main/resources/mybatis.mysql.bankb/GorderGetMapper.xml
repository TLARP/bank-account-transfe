<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.netease.kaola.generic.api.bankb.mapper.GorderGetMapper">
    <resultMap id="map" type="com.netease.kaola.generic.api.model.GorderRcords">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="use_name" property="userName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="baseColumn">id,username</sql>
    <insert id="insertGorderRcords" keyProperty="id" useGeneratedKeys="true">
        insert into g_order
        (<include refid="base"/>)
        VALUES
        (
        seq,
        #{userName}
        )
    </insert>
</mapper>
